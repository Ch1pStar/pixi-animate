{
	"name": "PixiAnimate",
	"version": "0.3.3",
	"description": "Runtime library to play Pixi.js content from Adobe Animate CC",
	"url": "https://github.com/jiborobot/pixi-animate",
	"output": "pixi-animate",
	"main": [
		"src/animate/main.js",
		"src/animate/load.js",
		"src/animate/ColorUtils.js",
		"src/animate/ShapesCache.js",
		"src/animate/SymbolLoader.js",
		"src/animate/Tween.js",
		"src/animate/Timeline.js",
		"src/animate/MovieClip.js",
		"src/mixins/*.js"
	],
	"modules":
	{
		"springroll": [
			"src/plugins/springroll/FlashArt.js",
			"src/plugins/springroll/FlashArtTask.js",
			"src/plugins/springroll/MovieClipInstance.js",
			"src/plugins/springroll/PixiAnimatePlugin.js",
			"src/plugins/springroll/SpriteSheetTask.js"
		]
	}
}